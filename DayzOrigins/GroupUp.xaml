<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
	x:Class="DayzOrigins.GroupUp"
	x:Name="Window"
	Title="Объединение групп"
	Width="292" Height="124" Icon="/AИС учебная часть;component/logo.ico">

	<Grid x:Name="LayoutRoot" Background="Gray">
		<TextBlock HorizontalAlignment="Left" Height="20" Margin="8,8,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="94"><Run Language="ru-ru" Text="Текущая группа"/></TextBlock>
		<TextBlock HorizontalAlignment="Right" Height="18" Margin="0,10,32,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="70"><Run Language="ru-ru" Text="Перевод в"/></TextBlock>
		<TextBox HorizontalAlignment="Left" Margin="8,32,0,0" TextWrapping="Wrap" Width="94" Height="20" VerticalAlignment="Top" Name="group_old" Background="#FFBABABA" BorderBrush="#FF707070" />
		<TextBox HorizontalAlignment="Right" Margin="0,32,8,0" TextWrapping="Wrap" Width="94" Height="20" VerticalAlignment="Top" Name="group_new" Background="#FFBABABA" BorderBrush="#FF707070" />
		<ed:BlockArrow Fill="#FFBABABA" Margin="106,32,106,0" Orientation="Right" Stroke="#FF707070" Height="20" VerticalAlignment="Top" ArrowheadAngle="55"/>
		<Button Content="Выполнить" Height="21" Margin="8,0,8,8" VerticalAlignment="Bottom" Click="Button_Click" Background="#FFBABABA" />
	</Grid>
</Window>